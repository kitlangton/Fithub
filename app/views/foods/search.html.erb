<h1>Results for <%=@food_search%></h1>
<div class="row">
  <div class="col-sm-12">
    <table class="table">
      <thead>
        <th>Name</th>
        <th>Calories</th>
      </thead>
      <tbody>
        <% @results.each do |result| %>
          <tr>
            <td><%= result.name %></td>
            <td><%= result.calories %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
